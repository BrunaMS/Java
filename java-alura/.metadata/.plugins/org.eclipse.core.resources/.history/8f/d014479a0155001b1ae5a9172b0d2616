
public class TestConditionals {
	public static void main(String[] args) {
		System.out.println("Testing conditionals");
		
		int age = 15;
		int anotherAge = 25;
		
		if(age >= 18) {
			System.out.println("You're more than 18 years");
			System.out.println("Welcome");
		}
		else {
			if(anotherAge >= 18) {
				System.out.println("Welcome!" + '\n +  "You can enter here because you're accompanied");
			}
			System.out.println("Unfortunately you aren't more than 18 years");
		}
	}
}
